   "source": [
    "#This is a program to print the fibonacci sequence upto nth term\n",
    "fib = [0, 1]\n",
    "n = int(input(\"Enter the number of terms to be displayed in the Fibonacci sequence:\"))\n",
    "for i in range(2,n):\n",
    "    fib.append(fib[i-1]+fib[i-2])\n",
    "for i in range(n):\n",
    "    print(fib[i])"
   ]
